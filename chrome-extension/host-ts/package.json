{
  "name": "@extension/native-host",
  "version": "0.4.3",
  "description": "Native messaging host for Hoarder extension",
  "type": "module",
  "private": true,
  "bin": {
    "native-host": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "install-host": "node dist/install.js",
    "uninstall-host": "node dist/install.js --uninstall"
  },
  "dependencies": {
    "chromadb": "^1.10.5"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "typescript": "^5.8.1-rc",
    "vitest": "^3.1.3"
  }
}
